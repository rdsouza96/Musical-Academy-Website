body{   background: Seashell;
        width: 1000px;
        margin-left: 140px;
        }

h1{     text-align: center;
        width: auto;
        margin-bottom: 10px;
        padding: 10px;
        border: 3px solid blue;
        color: #0C090A;
        }

ul{     list-style-type: none;
        margin: 0;
        padding: 0;
        overflow: hidden;
        background-color: #0C090A
        }

li{     float: left;
        }

li a{   display: block;
        padding: 14px 16px;
        text-align: center;
        color: white;
        text-decoration: none;
        }
li a:hover{     background: #726E6D;
                }

.active{background-color: black;
        }
